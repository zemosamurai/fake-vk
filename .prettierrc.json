{
  "printWidth": 120,
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^src/app/(.*)$",
    "^src/assets/(.*)$",
    "^src/common/(.*)$",
    "^src/features/(.*)$",
    "^src/pages/(.*)$",
    "^src/services/(.*)$",
    "^./(.*)",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}